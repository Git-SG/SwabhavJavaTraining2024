package com.aurionpro.dip.solution.model;

public class TaxCalculator {
	
	private ILogger logger;

	public TaxCalculator() {
	}
	
	public int calculateTax(int amount, int rate) {
		int tax = 0;
		try {
			tax = amount/rate;
			System.out.println(tax);
		}
		catch(Exception e) {
			logger.log("Divide by zero");
		}
		
		return tax;
	}

}
